{
    "manifest_version":2,
    "name": "Auto Book",
    "version": "2.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "./images.png"
    }
}